#. module: l10n_nl_reports
#. odoo-python
#: code:addons/l10n_nl_reports/models/l10n_nl_reports_sbr_status_service.py:0
msgid ""
"The status retrieval for the %(report_name)s with discussion id '%(id)s' "
"failed with the error:<br/><br/><i>%(error)s</i><br/><br/>Try submitting "
"your report again."
msgstr ""
"Het ophalen van de status voor de %(report_name)s met discussie-ID '%(id)s' "
"is mislukt met de fout:<br/><br/><i>%(error)s</i><br/><br/>Probeer uw "
"rapport opnieuw in te dienen."

#. module: l10n_nl_reports
#. odoo-python
#: code:addons/l10n_nl_reports/models/l10n_nl_reports_sbr_status_service.py:0
msgid ""
"The submission for the %(report_name)s with discussion id '%(id)s' failed "
"with the "
"error:<br/><br/><i>%(error)s</i><br/><i>%(detailed_error)s</i><br/><br/>Try "
"submitting your report again."
msgstr ""
"De indiening voor de %(report_name)s met discussie-id '%(id)s' is mislukt "
"met de fout:<br/><br/><i>%s</i><br/><i>%(error)s</i><br/><br/>Probeer uw "
"rapport opnieuw in te dienen."

#. module: l10n_nl_reports
#. odoo-python
#: code:addons/l10n_nl_reports/models/l10n_nl_reports_sbr_status_service.py:0
msgid ""
"The submission for the %(report_name)s with discussion id '%(id)s' was "
"successfully received by Digipoort."
msgstr ""
"De inzending voor de %(report_name)s met discussie id '%(id)s' is succesvol "
"ontvangen door Digipoort."

#. module: l10n_nl_reports
