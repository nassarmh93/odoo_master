<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="sale_commission_achievement_link_view_list" model="ir.ui.view">
        <field name="name">sale.commission.achievement.view.list</field>
        <field name="model">sale.commission.achievement</field>
        <field name="inherit_id" ref="sale_commission.sale_commission_achievement_view_list"/>
        <field name="arch" type="xml">
            <field name="team_id" position="replace"/>
             <field name="user_id" position="replace">
                <field name="add_user_id"/>
                <field name="reduce_user_id"/>
             </field>
            <field name="amount" position="replace">
                <field name="achieved" sum="Total Achieved"/>
            </field>
        </field>
    </record>
</odoo>
